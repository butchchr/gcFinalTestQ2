@model IEnumerable< FinalTestQ2.Order>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Orders</h4>
    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>OrderID</th>
            <th>OrderDate</th>
            <th>ShipCity</th>

        </tr>
        @foreach (Order order in Model)
        {
            <tr>
                <td>@order.OrderID</td>
                <td>@order.OrderDate</td>
                <td>@order.ShipCity</td>
            </tr>
        }
    </table>
</body>
</html>
